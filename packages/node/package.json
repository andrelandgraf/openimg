{
  "name": "openimg-node",
  "version": "0.0.6",
  "homepage": "https://github.com/andrelandgraf/openimg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrelandgraf/openimg.git",
    "directory": "packages/node"
  },
  "license": "MIT",
  "author": "Andre Landgraf <andre.timo.landgraf@gmail.com> (https://andrelandgraf.com)",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/types/*.d.ts"
    }
  },
  "scripts": {
    "build": "bun ./scripts/build.ts && bun run build:ts",
    "build:ts": "tsc"
  },
  "dependencies": {
    "openimg-server-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "sharp": "^0.33.5",
    "typescript": "^5.0.0"
  },
  "files": [
    "src",
    "dist"
  ]
}
